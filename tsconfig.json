{
	"compilerOptions": {
		"target": "ES2022",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": false,
		"outDir": "dist",
		"rootDir": "src",
		"isolatedModules": true,
		"allowUnusedLabels": false,
		"noFallthroughCasesInSwitch": true
	},
	"include": ["src"]
}
